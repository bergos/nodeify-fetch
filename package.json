{
  "name": "nodeify-fetch",
  "version": "3.0.0",
  "description": "Node streams for fetch",
  "type": "module",
  "main": "index.js",
  "browser": {
    "./index.js": "./browser.js",
    "./test/support/server.js": "./test/support/browser-server.js"
  },
  "scripts": {
    "build-test": "mkdir -p .build && webpack ./test/*.js -o .build --mode development",
    "coverage": "codecov",
    "start-test-server": "node ./test/support/serve.js",
    "test": "stricter-standard && c8 --reporter=lcov --reporter=text-summary mocha",
    "watch-test": "webpack -o .build --mode development --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bergos/nodeify-fetch.git"
  },
  "keywords": [
    "fetch",
    "readable",
    "stream"
  ],
  "author": "Thomas Bergwinkl <bergi@axolotlfarm.org> (https://www.bergnet.org/people/bergi/card#me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bergos/nodeify-fetch/issues"
  },
  "homepage": "https://github.com/bergos/nodeify-fetch",
  "dependencies": {
    "lodash": "^4.17.21",
    "node-fetch": "^3.0.0",
    "readable-error": "^1.0.0",
    "readable-stream": "^3.6.0"
  },
  "devDependencies": {
    "c8": "^7.10.0",
    "codecov": "^3.8.3",
    "express": "^4.17.1",
    "express-as-promise": "^1.2.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "promise-the-world": "^1.0.1",
    "stricter-standard": "^0.2.0",
    "webpack": "^5.58.0",
    "webpack-cli": "^4.9.0"
  }
}
